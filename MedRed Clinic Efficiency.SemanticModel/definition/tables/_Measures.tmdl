table _Measures
	lineageTag: 9c9e0174-f37f-424c-9c6a-8afa5ea96517

	measure 'Medida 1: Ingresos por Consultas' = SUMX(FILTER(Consultas, RELATED(Medicos[TarifaConsulta]) > 0), RELATED(Medicos[TarifaConsulta]))
		lineageTag: b85504e3-6138-4730-bbf3-9c505b253899

		annotation PBI_FormatHint = {"isGeneralNumber":true}

	measure 'Medida 2: Duración Promedio de Consulta' = AVERAGEX(ALL(Consultas), Consultas[Duracion_min])
		lineageTag: bca32347-7f20-4b3c-bfa2-fed4e9f4e291

		annotation PBI_FormatHint = {"isGeneralNumber":true}

	measure 'Medida 3: % Alta Médica' = DIVIDE(CALCULATE(COUNTROWS(Consultas), Consultas[Resultado] = "Alta"), COUNTROWS(Consultas), 0)
		lineageTag: 093d0949-204f-444a-9597-ba33712b86bc

		annotation PBI_FormatHint = {"isGeneralNumber":true}

